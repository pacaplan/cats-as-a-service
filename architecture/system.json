{
  "$schema": "rampart/schema/system_v1.json",
  "schema_version": "1.0",
  "system": {
    "id": "cat_alog",
    "name": "Cat-alog",
    "description": "AI-powered custom cat generation and premade cat catalog"
  },
  "rails_app": {
    "root": "apps/web"
  },
  "engines": {
    "base_dir": "engines",
    "items": [
      {
        "id": "cat_content",
        "engine_name": "cat_content",
        "path": "engines/cat_content",
        "architecture_file": "architecture/cat_content.json",
        "enabled": true
      }
    ]
  },
  "profiles": {
    "default": "rampart_hexddd_v1",
    "overrides": []
  },
  "discovery": {
    "architecture_filename": "cat_content.json",
    "strict_engine_list": true
  }
}
